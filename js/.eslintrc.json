{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 5
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-underscore-dangle": 0,
    "vars-on-top": 0,
    "padded-blocks": 0,
    "no-var": 0,
    "comma-dangle": 0,
    "func-names": 0,
    "prefer-arrow-callback": 0,
    "object-shorthand": 0,
    "no-unused-expressions": [2, {
      "allowTernary": true,
      "allowShortCircuit": true
    }],
    "global-require": 0
  },
  "globals": {
    "$": true,
    "_": true
  }
}
